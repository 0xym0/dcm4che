# Extended DICOM Application Configuration Data Model Hierarchy LDAP Schema
dn: cn=dcm4che,cn=schema,cn=config
objectClass: olcSchemaConfig
cn: dcm4che
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.1 NAME 'dcm4cheTLSNeedClientAuth'
  DESC 'Indicates if TLS client authentication is required'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.2 NAME 'dcm4cheTLSProtocol'
  DESC 'The supported TLS Protocols'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.3 NAME 'dcm4cheTCPBacklog'
  DESC 'Maximum queue length for incoming TCP connections'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.4 NAME 'dcm4cheTCPConnectTimeout'
  DESC 'TCP connect timeout in ms'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.5 NAME 'dcm4cheAssociationRequestTimeout'
  DESC 'Timeout in ms for receive of A-ASSOCIATE-RQ PDU after TCP connect'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.6 NAME 'dcm4cheAssociationAcknowledgeTimeout'
  DESC 'Timeout in ms for receive of A-ASSOCIATE-AC PDU after send of A-ASSOCIATE-RQ PDU'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.7 NAME 'dcm4cheAssociationReleaseTimeout'
  DESC 'Timeout in ms for receive of A-RELEASE-RP PDU after send of A-RELEASE-RQ PDU'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.8 NAME 'dcm4cheDIMSEResponseTimeout'
  DESC 'Timeout in ms for receive of DIMSE-RSP after send of DIMSE-RQ'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.9 NAME 'dcm4cheCMoveResponseTimeout'
  DESC 'Timeout in ms for receive of C-MOVE-RSP after send of C-MOVE-RQ'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.10 NAME 'dcm4cheCGetResponseTimeout'
  DESC 'Timeout in ms for receive of C-GET-RSP after send of C-GET-RQ'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.11 NAME 'dcm4cheAssociationIdleTimeout'
  DESC 'Indicates aborting of idle Associations afer specified timeout in ms'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.12 NAME 'dcm4cheTCPCloseDelay'
  DESC 'TCP socket close delay in ms after send of A-ASSOCIATE-RJ, A-RELEASE-RP or A-ABORT PDU'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.13 NAME 'dcm4cheTCPSendBufferSize'
  DESC 'TCP send buffer size'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.14 NAME 'dcm4cheTCPReceiveBufferSize'
  DESC 'TCP receive buffer size'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.15 NAME 'dcm4cheTCPNoDelay'
  DESC 'Enable/disable TCP_NODELAY (disable/enable Nagle algorithm)'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.16 NAME 'dcm4cheBlacklistedHostname'
  DESC 'blacklisted DNS hostnames'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.17 NAME 'dcm4cheSendPDULength'
  DESC 'Maximal length of emitted PDUs'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.18 NAME 'dcm4cheReceivePDULength'
  DESC 'Maximal length of received PDUs'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.19 NAME 'dcm4cheMaxOpsPerformed'
  DESC 'Maximal number of operations to perform asynchronously'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.20 NAME 'dcm4cheMaxOpsInvoked'
  DESC 'Maximal number of operations to invoke asynchronously'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.21 NAME 'dcm4chePackPDV'
  DESC 'Enable/disable packing of command and data PDVs into one P-DATA-TF PDU'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.22 NAME 'dcm4cheAcceptOnlyPreferredCallingAETitle'
  DESC 'Enable/disable check of Calling AE Title in Association requests'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.23 NAME 'dcm4cheRelationalQueries'
  DESC 'Enable/disable relational queries'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.24 NAME 'dcm4cheCombinedDateTimeMatching'
  DESC 'Enable/disable combined date time matching'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.25 NAME 'dcm4cheFuzzySemanticMatching'
  DESC 'Enable/disable fuzzy semantic matching of person names'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.26 NAME 'dcm4cheTimezoneQueryAdjustment'
  DESC 'Enable/disable timezone query adjustment'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.27 NAME 'dcm4cheLevelOfStorageConformance'
  DESC 'Indicates level of Conformance of a Storage SCP'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.28 NAME 'dcm4cheLevelOfDigitalSignatureSupport'
  DESC 'Indicates level of Digital Signature support of a Storage SCP'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.29 NAME 'dcm4cheDataElementCoercion'
  DESC 'Indicates coercion of Data Elements of a Storage SCP'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcObjectClasses: ( 1.2.40.0.13.1.15.0.4.5 NAME 'dcm4cheNetworkAE'
  DESC 'Extended DICOM Network AE related information'
  SUP dicomNetworkAE STRUCTURAL
  MAY (
    dcm4cheSendPDULength $
    dcm4cheReceivePDULength $
    dcm4cheMaxOpsPerformed $
    dcm4cheMaxOpsInvoked $
    dcm4chePackPDV $
    dcm4cheAcceptOnlyPreferredCallingAETitle ) )
olcObjectClasses: ( 1.2.40.0.13.1.15.0.4.6 NAME 'dcm4cheNetworkConnection'
  DESC 'Extended DICOM Network Connection information'
  SUP dicomNetworkConnection STRUCTURAL
  MAY (
    dcm4cheTLSNeedClientAuth $
    dcm4cheTLSProtocol $
    dcm4cheTCPBacklog $
    dcm4cheTCPConnectTimeout $
    dcm4cheTCPCloseDelay $
    dcm4cheTCPSendBufferSize $
    dcm4cheTCPReceiveBufferSize $
    dcm4cheTCPNoDelay $
    dcm4cheBlacklistedHostname $
    dcm4cheAssociationRequestTimeout $
    dcm4cheAssociationAcknowledgeTimeout $
    dcm4cheAssociationReleaseTimeout $
    dcm4cheDIMSEResponseTimeout $
    dcm4cheCGetResponseTimeout $
    dcm4cheCMoveResponseTimeout $
    dcm4cheAssociationIdleTimeout ) )
olcObjectClasses: ( 1.2.40.0.13.1.15.0.4.8 NAME 'dcm4cheTransferCapability'
  DESC 'Extended Transfer Capabilities for an Application Entity'
  SUP dicomTransferCapability STRUCTURAL
  MAY (
     dcm4cheRelationalQueries $
     dcm4cheCombinedDateTimeMatching $
     dcm4cheFuzzySemanticMatching $
     dcm4cheTimezoneQueryAdjustment $
     dcm4cheLevelOfStorageConformance $
     dcm4cheLevelOfDigitalSignatureSupport $
     dcm4cheDataElementCoercion ) )
