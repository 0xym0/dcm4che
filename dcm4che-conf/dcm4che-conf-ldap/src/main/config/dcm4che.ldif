# Extended DICOM Application Configuration Data Model Hierarchy LDAP Schema
dn: cn=dicom,cn=schema,cn=config
objectClass: olcSchemaConfig
cn: dicom
olcAttributeTypes: ( 1.2.840.10008.15.0.3.1 NAME 'dicomDeviceName'
  DESC 'The unique name for the device'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.2 NAME 'dicomDescription'
  DESC 'Textual description of the DICOM entity'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.3 NAME 'dicomManufacturer'
  DESC 'The device Manufacturer name'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.4 NAME 'dicomManufacturerModelName'
  DESC 'The device Manufacturer Model Name'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.5 NAME 'dicomSoftwareVersion'
  DESC 'The device software version. Should be the same as the values of Software Versions (0018,1020) in SOP instances created by this device.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.6 NAME 'dicomVendorData'
  DESC 'Arbitrary vendor-specific configuration information (binary data)'
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.5 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.7 NAME 'dicomAETitle'
  DESC 'Application Entity (AE) title'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.8 NAME 'dicomNetworkConnectionReference'
  DESC 'The DN of a dicomNetworkConnection object used by an Application Entity'
  EQUALITY distinguishedNameMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.12 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.9 NAME 'dicomApplicationCluster'
  DESC 'Application cluster name for an Application Entity (e.g. "Neuroradiology Research")'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.10 NAME 'dicomAssociationInitiator'
  DESC 'Indicates if an Application Entity is capable of initiating network associations'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.11 NAME 'dicomAssociationAcceptor'
  DESC 'Indicates if an Application Entity is capable of accepting network associations'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.12 NAME 'dicomHostname'
  DESC 'DNS hostname'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.13 NAME 'dicomPort'
  DESC 'TCP Port number'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.14 NAME 'dicomSOPClass'
  DESC 'A SOP Class UID'
  EQUALITY objectIdentifierMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.38
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.15 NAME 'dicomTransferRole'
  DESC 'Transfer role (either "SCU" or "SCP")'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.16 NAME 'dicomTransferSyntax'
  DESC 'A Transfer Syntax UID'
  EQUALITY objectIdentifierMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.38 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.17 NAME 'dicomPrimaryDeviceType'
  DESC 'The device Primary Device type'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.18 NAME 'dicomRelatedDeviceReference'
  DESC 'The DN of a related device description outside the DICOM Configuration Hierachy'
  EQUALITY distinguishedNameMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.12 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.19 NAME 'dicomPreferredCalledAETitle'
  DESC 'AE Title(s) to which associations may be preferably initiated.'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.20 NAME 'dicomTLSCipherSuite'
  DESC 'The supported TLS CipherSuites'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.21 NAME 'dicomAuthorizedNodeCertificateReference'
  DESC 'The DN of a Certificate for a DICOM node that is authorized to connect to this node'
  EQUALITY distinguishedNameMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.12 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.22 NAME 'dicomThisNodeCertificateReference'
  DESC 'The DN of a related device description outside the DICOM Configuration Hierachy'
  EQUALITY distinguishedNameMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.12 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.23 NAME 'dicomInstalled'
  DESC 'Indicates if the DICOM object (device, Network AE, or Port) is presently installed'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.24 NAME 'dicomStationName'
  DESC 'Station Name of the device.  Should be the same as the value of Station Name (0008,1010) in SOP instances created by this device.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.25 NAME 'dicomDeviceSerialNumber'
  DESC 'Serial number of the device. Should be the same as the value of Device Serial Number (0018,1000) in SOP instances created by this device.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.26 NAME 'dicomInstitutionName'
  DESC 'Institution name of the device. Should be the same as the value of Institution Name (0008,0080) in SOP Instances created by this device.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.27 NAME 'dicomInstitutionAddress'
  DESC 'Institution address of the device.  Should be the same as the value of Institution Address (0008,0081) attribute in SOP Instances created by this device.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.28 NAME 'dicomInstitutionDepartmentName'
  DESC 'Institution department name of the device.  Should be the same as the value of Institutional Department Name (0008,1040) in SOP Instances created by this device.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.29 NAME 'dicomIssuerOfPatientID'
  DESC 'Default value for the Issuer of Patient ID (0010,0021) for SOP Instances created by this device.  May be overridden by the values received in a worklist or other source.'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.30 NAME 'dicomPreferredCallingAETitle'
  DESC 'AE Title(s) to which associations may be preferably accepted.'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
olcAttributeTypes: ( 1.2.840.10008.15.0.3.31 NAME 'dicomSupportedCharacterSet'
  DESC 'The Character Set(s) supported by the Network AE for data sets it receives.'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.1 NAME 'dicomTLSNeedClientAuth'
  DESC 'Indicates if TLS client authentication is required'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.2 NAME 'dicomTLSProtocol'
  DESC 'The supported TLS Protocols'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.3 NAME 'dicomTCPBacklog'
  DESC 'Maximum queue length for incoming TCP connections'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.4 NAME 'dicomTCPConnectTimeout'
  DESC 'TCP connect timeout in ms'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.5 NAME 'dicomAssociationRequestTimeout'
  DESC 'Timeout in ms for receive of A-ASSOCIATE-RQ PDU after TCP connect'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.6 NAME 'dicomAssociationAcknowledgeTimeout'
  DESC 'Timeout in ms for receive of A-ASSOCIATE-AC PDU after send of A-ASSOCIATE-RQ PDU'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.7 NAME 'dicomAssociationReleaseTimeout'
  DESC 'Timeout in ms for receive of A-RELEASE-RP PDU after send of A-RELEASE-RQ PDU'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.8 NAME 'dicomDIMSEResponseTimeout'
  DESC 'Timeout in ms for receive of DIMSE-RSP after send of DIMSE-RQ'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.9 NAME 'dicomCMoveResponseTimeout'
  DESC 'Timeout in ms for receive of C-MOVE-RSP after send of C-MOVE-RQ'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.10 NAME 'dicomCGetResponseTimeout'
  DESC 'Timeout in ms for receive of C-GET-RSP after send of C-GET-RQ'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.11 NAME 'dicomAssociationIdleTimeout'
  DESC 'Indicates aborting of idle Associations afer specified timeout in ms'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.12 NAME 'dicomTCPCloseDelay'
  DESC 'TCP socket close delay in ms after send of A-ASSOCIATE-RJ, A-RELEASE-RP or A-ABORT PDU'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.13 NAME 'dicomTCPSendBufferSize'
  DESC 'TCP send buffer size'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.14 NAME 'dicomTCPReceiveBufferSize'
  DESC 'TCP receive buffer size'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.15 NAME 'dicomTCPNoDelay'
  DESC 'Enable/disable TCP_NODELAY (disable/enable Nagle algorithm)'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.16 NAME 'dicomBlacklistedHostname'
  DESC 'DNS hostname'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.17 NAME 'dicomSendPDULength'
  DESC 'Maximal length of emitted PDUs'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.18 NAME 'dicomReceivePDULength'
  DESC 'Maximal length of received PDUs'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.19 NAME 'dicomMaxOpsPerformed'
  DESC 'Maximal number of operations to perform asynchronously'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.20 NAME 'dicomMaxOpsInvoked'
  DESC 'Maximal number of operations to invoke asynchronously'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.0.1.21 NAME 'dicomPackPDV'
  DESC 'Enable/disable packing of command and data PDVs into one P-DATA-TF PDU'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcObjectClasses: ( 1.2.840.10008.15.0.4.1 NAME 'dicomConfigurationRoot'
  DESC 'Root of the DICOM Configuration Hierarchy'
  SUP top STRUCTURAL
  MUST cn
  MAY description )
olcObjectClasses: ( 1.2.840.10008.15.0.4.2 NAME 'dicomDevicesRoot'
  DESC 'Root of the DICOM Devices Hierarchy'
  SUP top STRUCTURAL
  MUST cn
  MAY description )
olcObjectClasses: ( 1.2.840.10008.15.0.4.3 NAME 'dicomUniqueAETitlesRegistryRoot'
  DESC 'Root of the Unique DICOM AE-Title Registry Hierarchy'
  SUP top STRUCTURAL
  MUST cn
  MAY description )
olcObjectClasses: ( 1.2.840.10008.15.0.4.4 NAME 'dicomDevice'
  DESC 'DICOM Device related information'
  SUP top STRUCTURAL
  MUST (
    dicomDeviceName $
    dicomInstalled )
  MAY (
    dicomDescription $
    dicomManufacturer $
    dicomManufacturerModelName $
    dicomSoftwareVersion $
    dicomStationName $
    dicomDeviceSerialNumber $
    dicomInstitutionName $
    dicomInstitutionAddress $
    dicomInstitutionDepartmentName $
    dicomIssuerOfPatientID $
    dicomVendorData $
    dicomPrimaryDeviceType $
    dicomRelatedDeviceReference $
    dicomAuthorizedNodeCertificateReference $ 
    dicomThisNodeCertificateReference ) )
olcObjectClasses: ( 1.2.840.10008.15.0.4.5 NAME 'dicomNetworkAE'
  DESC 'DICOM Network AE related information'
  SUP top STRUCTURAL
  MUST (
    dicomAETitle $
    dicomNetworkConnectionReference $
    dicomAssociationInitiator $
    dicomAssociationAcceptor )
  MAY (
    dicomDescription $
    dicomVendorData $
    dicomApplicationCluster $
    dicomPreferredCalledAETitle $
    dicomPreferredCallingAETitle $
    dicomSupportedCharacterSet $
    dicomSendPDULength $
    dicomReceivePDULength $
    dicomMaxOpsPerformed $
    dicomMaxOpsInvoked $
    dicomPackPDV $
    dicomInstalled ) )
olcObjectClasses: ( 1.2.840.10008.15.0.4.6 NAME 'dicomNetworkConnection'
  DESC 'DICOM Network Connection information'
  SUP top STRUCTURAL
  MUST dicomHostname
  MAY (
    cn $
    dicomPort $
    dicomTLSCipherSuite $
    dicomTLSNeedClientAuth $
    dicomTLSProtocol $
    dicomTCPBacklog $
    dicomTCPConnectTimeout $
    dicomTCPCloseDelay $
    dicomTCPSendBufferSize $
    dicomTCPReceiveBufferSize $
    dicomTCPNoDelay $
    dicomBlacklistedHostname $
    dicomAssociationRequestTimeout $
    dicomAssociationAcknowledgeTimeout $
    dicomAssociationReleaseTimeout $
    dicomDIMSEResponseTimeout $
    dicomCGetResponseTimeout $
    dicomCMoveResponseTimeout $
    dicomAssociationIdleTimeout $
    dicomInstalled ) )
olcObjectClasses: ( 1.2.840.10008.15.0.4.7 NAME 'dicomUniqueAETitle'
  DESC 'A Unique DICOM Application Entity title'
  SUP top STRUCTURAL
  MUST dicomAETitle)
olcObjectClasses: ( 1.2.840.10008.15.0.4.8 NAME 'dicomTransferCapability'
  DESC 'Transfer Capabilities for an Application Entity'
  SUP top STRUCTURAL
  MUST (
    dicomSOPClass $
    dicomTransferRole $
    dicomTransferSyntax )
  MAY cn )
